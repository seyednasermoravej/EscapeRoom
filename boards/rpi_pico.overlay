	// 		status = "okay";
	// 		color_sensor0: tcs34725@29{
	// 			compatible = "taos,tcs34725"
	// 			reg = <0x29>;
	// 		};
			
	// 	};
	// };

};


&i2c1{
	status = "okay";
	pinctrl-0 = <&i2c1_default>;
	clock-frequency = <I2C_BITRATE_STANDARD>;
		lcd1: pcf857x@20 {
		compatible = "nxp,pcf857x";
		status = "okay";
		reg = <0x20>;
		gpio-controller;
		#gpio-cells = <2>;
		ngpios = <8>;
		zephyr,deferred-init;
	};
		lcd2: pcf857x@21{
		compatible = "nxp,pcf857x";
		reg = <0x21>;
		gpio-controller;
		#gpio-cells = <2>;
		ngpios = <8>;
		zephyr,deferred-init;
	};
		expander: pcf857x@27 {
		compatible = "nxp,pcf857x";
		status = "okay";
		reg = <0x27>;
		gpio-controller;
		#gpio-cells = <2>;
		ngpios = <8>;
		zephyr,deferred-init;
	};
  

};


