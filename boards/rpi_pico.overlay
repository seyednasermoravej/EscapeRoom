				gpios = <&gpio0 14 (GPIO_ACTIVE_HIGH)>;
			};
			platform_stepper_direction: stepper-direction{
				compatible = "power-switch";
				gpios = <&gpio0 15 (GPIO_ACTIVE_HIGH)>;
			};
		};
	};

