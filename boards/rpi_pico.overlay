	// 	spi-max-frequency = <25000000>;
	// 	int-gpios = <&gpio0 21 GPIO_ACTIVE_LOW>;
	// 	reset-gpios = <&gpio0 20 GPIO_ACTIVE_LOW>;
	// 	local-mac-address = [00 00 00 01 02 03];
	// 	status = "okay";
	// };
};


&spi1 {
	clock-frequency = <DT_FREQ_M(20)>;
	status = "okay";
	pinctrl-0 = <&spi1_pins>;
	pinctrl-names = "default";
	cs-gpios = <&gpio0 13 GPIO_ACTIVE_LOW>;

};



pico_spi: &spi0 {};
&pwm{
	status = "okay";
	pinctrl-0 = <&pwm_pins>;
// 	divider-int-0 = <133>;

	// divider-int-1 = <133>;

// 	divider-int-2 = <133>;

// 	divider-int-3 = <133>;

	divider-int-4 = <133>;

	divider-int-5 = <133>;

// 	divider-int-6 = <133>;

// 	divider-int-7 = <133>;

	zephyr,deferred-init;

};

&pinctrl{
	pwm_pins: pwm_pins
	{
		group1{
			pinmux = <PWM_4A_P8>, <PWM_4B_P9>, <PWM_5A_P26>, <PWM_5B_P27>;
		};
	};
	spi1_pins: spi1_pins {
		group1 {
			pinmux = <SPI1_CSN_P13>, <SPI1_SCK_P10>, <SPI1_TX_P11>;
		};
		group2 {
			pinmux = <SPI1_RX_P12>;
			input-enable;
		};
	};

};

&i2c1_default {
	group1 {
		pinmux = <I2C1_SDA_P14>, <I2C1_SCL_P15>;

	};

  };


&i2c0{
	status = "okay";
	pinctrl-0 = <&i2c0_default>;
	rfidin: pn532@24{
		compatible = "nxp,pn532";
		status = "okay";
		reg = <0x24>;
		int-gpios = <&gpio0 2 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
		reset-gpios = <&gpio0 3 (GPIO_ACTIVE_LOW)>;
	};

	// mux0: tca9546a@77
	// {
