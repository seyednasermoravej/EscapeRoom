	shelf
	{

	};

		buydisplay_3_5_tft_touch_arduino_mipi_dbi {
		compatible = "zephyr,mipi-dbi-spi";
		//////////////////
		//dc-gpios = <&arduino_header 13 GPIO_ACTIVE_HIGH>; /* D7 */
		//reset-gpios = <&arduino_header 16 GPIO_ACTIVE_LOW>; /* D10 */

		//spi-dev = <&arduino_spi>;
		
