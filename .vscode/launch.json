{
    "version": "0.2.0",
    "configurations": [
 
        {
            "name": "Launch",
            "cwd": "${workspaceFolder}",
            "executable": "build/zephyr/zephyr.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",

            "gdbPath": "${userHome}/zephyr-sdk-0.16.6/arm-zephyr-eabi/bin/arm-zephyr-eabi-gdb",
            "preLaunchTask": "buildFlash",
            "interface": "swd",
            "configFiles": [ "/home/naser/openocd/tcl/interface/ftdi/minimodule-swd.cfg", "/home/naser/openocd/tcl/target/rp2040.cfg"],
            "showDevDebugOutput": "raw",
            "rtos": "Zephyr",
            // "breakAfterReset": true
        }, 
 
    ]
}